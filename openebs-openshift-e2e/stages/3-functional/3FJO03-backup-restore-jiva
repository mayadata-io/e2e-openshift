cd jiva-operator/e2e-tests
cp apps/percona/deployers/run_e2e_test.yml percona_backup_restore_jiva.yml

sed -i -e 's/app: percona-deployment/app: deploy-percona-backup-restore-jiva/g' \
-e 's/value: openebs-jiva-csi/value: jiva-csi-sc/g' \
-e 's/value: app-percona-ns/value: backup-percona-jiva/g' percona_backup_restore_jiva.yml

cat percona_backup_restore_jiva.yml

bash ../../openebs-openshift-e2e/utils/e2e_job_runner label='app:deploy-percona-backup-restore-jiva' job=percona_backup_restore_jiva.yml
cd ../..





cd jiva-operator/e2e-tests
cp experiments/functional/backup_and_restore/run_e2e_test.yml run_backuprestore_jiva_test.yml

sed -i -e 's/value: app-percona-ns/value: backup-percona-jiva/g' \
-e 's/app: velero-backup-restore/app: velero-backup-restore-jiva/g' \
-e 's/value: percona-backup/value: percona-backup-jiva/g' run_backuprestore_jiva_test.yml

sed -i "s/v1.1.0/v1.9.0/g" run_backuprestore_jiva_test.yml

cat run_backuprestore_jiva_test.yml
bash ../../openebs-openshift-e2e/utils/e2e_job_runner label='app:velero-backup-restore-jiva' job=run_backuprestore_jiva_test.yml
cd ../..
