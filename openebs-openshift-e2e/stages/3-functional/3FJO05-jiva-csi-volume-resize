cd jiva-operator/e2e-tests
cp apps/busybox/deployers/run_e2e_test.yml deploy_csi_busybox.yml

sed -i -e 's/value: app-busybox-ns/value: busybox-csi/g' \
-e 's/app: busybox-e2e/app: busybox-e2e-csi/g' \
-e 's/value: openebs-jiva-csi/value: jiva-csi-sc/g' \
-e 's/generateName: e2e-busybox-deploy-/generateName: e2e-busybox-csi-/g' deploy_csi_busybox.yml

cat deploy_csi_busybox.yml

bash ../../openebs-openshift-e2e/utils/e2e_job_runner label='app:busybox-e2e-csi' job=deploy_csi_busybox.yml
cd ../..




cd jiva-operator/e2e-tests
cp apps/busybox/liveness/run_e2e_test.yml liveness_csi_busybox.yml

sed -i -e 's/liveness: e2e-busybox-liveness/liveness: e2e-busybox-csi-liveness/g' \
-e 's/generateName: e2e-busybox-liveness-/generateName: e2e-busybox-csi-liveness-/g' \
-e 's/value: app-busybox-ns/value: busybox-csi/g' liveness_csi_busybox.yml

cat liveness_csi_busybox.yml

bash ../../openebs-openshift-e2e/utils/e2e_job_runner label='liveness:e2e-busybox-csi-liveness' job=liveness_csi_busybox.yml
cd ../..




cd jiva-operator/e2e-tests
cp experiments/functional/csi-volume-resize/run_e2e_test.yml run_jiva_volume_resize_test.yml

sed -i -e '/name: APP_PVC/{n;s/.*/            value: openebs-busybox/g}' \
-e '/name: APP_LABEL/{n;s/.*/            value: app=busybox/g}' \
-e '/name: STORAGE_CLASS/{n;s/.*/            value: jiva-csi-sc/g}' \
-e '/name: PV_CAPACITY/{n;s/.*/            value: 5Gi/g}' \
-e '/name: NEW_CAPACITY/{n;s/.*/            value: 10Gi/g}' \
-e '/name: APP_NAMESPACE/{n;s/.*/            value: busybox-csi/g}' run_jiva_volume_resize_test.yml

cat run_jiva_volume_resize_test.yml

bash ../../openebs-openshift-e2e/utils/e2e_job_runner label='app:csi-vol-resize' job=run_jiva_volume_resize_test.yml
cd ../..





cd jiva-operator/e2e-tests
cp apps/busybox/liveness/run_e2e_test.yml dep_liveness_csi_busybox.yml

sed -i -e 's/liveness: e2e-busybox-liveness/liveness: e2e-busybox-csi-liveness-dep/g' \
-e 's/generateName: e2e-busybox-liveness-/generateName: e2e-busybox-csi-liveness-dep-/g' \
-e 's/value: provision/value: deprovision/g' \
-e 's/value: app-busybox-ns/value: busybox-csi/g' dep_liveness_csi_busybox.yml

cat dep_liveness_csi_busybox.yml

bash ../../openebs-openshift-e2e/utils/e2e_job_runner label='liveness:e2e-busybox-csi-liveness-dep' job=dep_liveness_csi_busybox.yml
cd ../..
bash openebs-openshift-e2e/utils/dump_cluster_state;
bash openebs-openshift-e2e/utils/event_updater jobname:csi-volume-resize $test_name jobid:"$job_id" pipelineid:"$pipeline_id" testcaseid:"$case_id"






cd jiva-operator/e2e-tests
cp apps/busybox/deployers/run_e2e_test.yml dep_csi_busybox.yml

sed -i -e 's/value: app-busybox-ns/value: busybox-csi/g' \
-e 's/app: busybox-e2e/app: busybox-e2e-csi-dep/g' \
-e 's/generateName: e2e-busybox-deploy-/generateName: e2e-busybox-csi-dep-/g' \
-e 's/value: statefulset/value: deployment/g' \
-e 's/value: provision/value: deprovision/g' \
-e 's/value: openebs-jiva-csi/value: jiva-csi-sc/g' dep_csi_busybox.yml

cat dep_csi_busybox.yml

bash ../../openebs-openshift-e2e/utils/e2e_job_runner label='app:busybox-e2e-csi-dep' job=dep_csi_busybox.yml
cd ../..
