cd jiva-operator/e2e-tests
cp experiments/functional/data-integrity/run_e2e_test.yml data_integrity_test_jiva.yml

sed -i -e 's/imagePullPolicy: Always/imagePullPolicy: IfNotPresent/g' \
-e 's/app: fio-di-e2e/app: fio-di-e2e-jiva/g' \
-e 's/value: openebs-jiva-csi/value: jiva-csi-sc/g' \
-e 's/value: fio/value: fio-jiva/g' data_integrity_test_jiva.yml

cat data_integrity_test_jiva.yml

bash ../../openebs-openshift-e2e/utils/e2e_job_runner label='app:fio-di-e2e-jiva' job=data_integrity_test_jiva.yml
cd ../..
